#include <stdio.h>

int main(void) {
int cod_peca1, cod_peca2, quant_peca1, quant_peca2;
float valor_peca1, valor_peca2, total;

scanf("%d %d %f",&cod_peca1, &quant_peca1, &valor_peca1);

scanf("%d %d %f",&cod_peca2, 
&quant_peca2, &valor_peca2);

total = (quant_peca1 * valor_peca1) + (quant_peca2 * valor_peca2);
	
printf("VALOR A PAGAR: R$ %f" , total);

return 0;
}
